<?xml version="1.0"?>

<PropertyList>

<path>oilpress.ac</path>

    <animation>
        <type>material</type>
        <object-name>AMPgauge_001</object-name>
        <object-name>AMPneedle</object-name>
        <emission>
             <!-- <red>0.05</red>
            <green>0.02</green>
            <blue>0.01</blue> -->
            <red>   0.25 </red>
            <green> 0.25 </green>
            <blue>  0.25 </blue>
			<factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

<!--
temp degrees
F to C

122 - 50
176 - 80
212 - 100
230 - 110
248 - 120
302 - 150

-->

    <animation>
        <type>rotate</type>
        <object-name>AMPneedle</object-name>
        <property>/engines/engine[0]/oil-pressure-psi</property>
        <interpolation>
        	<entry><ind> 0</ind><dep> -70</dep></entry>
			<entry><ind> 1</ind><dep> -60</dep></entry>
			<entry><ind> 20</ind><dep> -45</dep></entry>
			<entry><ind> 40</ind><dep> -19</dep></entry>
			<entry><ind> 50</ind><dep> 0</dep></entry>
			<entry><ind> 60</ind><dep> 19</dep></entry>
			<entry><ind> 80</ind><dep> 45</dep></entry>
			<entry><ind> 100</ind><dep> 60</dep></entry>
		</interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.006</z-m>
        </center>
    </animation>

</PropertyList>
