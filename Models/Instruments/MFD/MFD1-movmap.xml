<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	
<model>
	<path>MFD1-movmap.ac</path>
</model>
 
 
<animation>
<type>select</type>
<object-name>MFD.screen</object-name>
  <condition>
    <property>/controls/power/battery</property>
    <equals>
      <property>controls/mfd/mfdpage</property>
      <value>1</value>
    </equals>
  </condition>
</animation>

<animation>
        <type>material</type>
        <object-name>MFD.screen</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop>controls/mfd/displays/brightness-mm</factor-prop>
        </emission>
</animation>

