  <animation>
      <type>pick</type>
      <object-name>g3_button.002</object-name>
      <visible>true</visible>
      <action>
        <button>0</button>
        <repeatable>false</repeatable>
        <binding>
          <condition>
            <equals>
            <property>controls/mfd/mfd1page</property>
            <value>2</value>
            </equals>
          </condition>
          <command>property-cycle</command>
          <property>/instrumentation/efis/mfd/display-mode</property>
          <value>MAP</value>
          <value>VOR</value>
          <!-- <value>APP</value> -->
          <!-- <value>PLAN</value> -->
        </binding>
        <binding>
          <command>nasal</command>
          <script>switchclicksound.click("switch")</script>
        </binding>
        <binding>
            <command>property-toggle</command>
              <property>cavalon-915is/mfd1/buttons/btn.002</property>
        </binding>
        <mod-up>
          <binding>
            <command>property-toggle</command>
              <property>cavalon-915is/mfd1/buttons/btn.002</property>
          </binding>
        </mod-up>
      </action>
      <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>mfd1-b2</tooltip-id>
                <label>Mode</label>
              <condition>
                <equals>
                  <property>controls/mfd/mfd1page</property>
                  <value>2</value>
                </equals>
              </condition>
            </binding>
      </hovered>
    </animation>

  <animation>
    <type>pick</type>
    <object-name>g3_button.003</object-name>
    <visible>true</visible>
